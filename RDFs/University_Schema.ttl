@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dbo: <http://dbpedia.org/ontology>.
@prefix foaf: <http://xmlns.com/foaf/.1/>.
@prefix wd: <https://wwww.wikidata.org/wiki/>.
@prefix university: <http://example.org/university/> .


#Creating Classes
university:University a rdfs:Class;
    rdfs:label "university";
    rdfs:comment "A university."@en.

#Properties
university:wikidataEntry a rdf:Property;
    rdfs:label "has Wikidata Entry";
    rdfs:domain university:University;
    rdfs:range foaf:Document;
    rdfs:comment "Link to university wikidata entry."@en .


university:dbpediaEntry a rdf:Property;
    rdfs:label "has DBpedia Entry";
    rdfs:domain university:University; # permit only Universities
    rdfs:range foaf:Document;
    rdfs:comment "Link to university DBpedia enrty."@en .

#Linking Wikidata and DBpedia
<https://www.example.org/university/ConcordiaUniversity> a university:University;
    rdfs:label "Concordia University";
    university:WikidataEntry wd:Q326342;
    university:DBpediaEntry <https://dbpedia.org/page/Concordia_University>.




