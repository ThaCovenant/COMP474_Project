@prefix completedCourse: <http://example.org/completedCourse/> .
@prefix course: <http://example.org/course/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lecture: <http://example.org/lecture/> .
@prefix lectureContent: <http://example.org/lectureContent/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix student: <http://example.org/student/> .
@prefix topic: <http://example.org/topic/> .
@prefix university: <http://example.org/university/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

completedCourse:CompletedCourse a rdfs:Class .

course:Course a rdfs:Class ;
    rdfs:label "course" ;
    rdfs:comment "A course offered by the university."@en ;
    rdfs:seeAlso foaf:Document .

lecture:Assignments a rdfs:Class ;
    rdfs:label "Lecture Assignments" ;
    rdfs:comment "Assignments of lecture content."@en ;
    rdfs:subClassOf lectureContent:LectureContent .

lecture:Other a rdfs:Class ;
    rdfs:label "Other Materials" ;
    rdfs:comment "Other materials related to Lecture Content."@en ;
    rdfs:subClassOf lectureContent:LectureContent .

lecture:Readings a rdfs:Class ;
    rdfs:label "Lecture Readings" ;
    rdfs:comment "Readings of lecture: book chapters, web pages, etc."@en ;
    rdfs:subClassOf lectureContent:LectureContent .

lecture:Slides a rdfs:Class ;
    rdfs:label "Lecture Slides" ;
    rdfs:comment "Slides of lecture content."@en ;
    rdfs:subClassOf lectureContent:LectureContent .

lecture:Worksheets a rdfs:Class ;
    rdfs:label "Lecture Worksheets" ;
    rdfs:comment "Worksheets of lecture content."@en ;
    rdfs:subClassOf lectureContent:LectureContent .

lecture:lectures a rdfs:Class ;
    rdfs:seeAlso foaf:Document .

lectureContent:lectureContent a rdfs:Class ;
    rdfs:label "Lecture Content" ;
    rdfs:comment "Content of lecture."@en .

student:Student a rdfs:Class .

completedCourse:hasCompletionDate a rdf:Property ;
    rdfs:domain completedCourse:CompletedCourse ;
    rdfs:range xsd:date .

course:courseCredit a rdf:Property ;
    rdfs:label "Course Credits" ;
    rdfs:comment "The number of credits assigned to the course."@en ;
    rdfs:domain course:Course ;
    rdfs:range xsd:integer .

course:courseDescription a rdf:Property ;
    rdfs:label "Course Description" ;
    rdfs:comment "Description of the course."@en ;
    rdfs:domain course:Course ;
    rdfs:range rdfs:Literal .

course:courseName a rdf:Property ;
    rdfs:label "Course name" ;
    rdfs:comment "Name of the course."@en ;
    rdfs:domain course:Course ;
    rdfs:range rdfs:Literal .

course:courseNumber a rdf:Property ;
    rdfs:label "Course number" ;
    rdfs:comment "Number of the course."@en ;
    rdfs:domain course:Course ;
    rdfs:range rdfs:Literal .

course:courseOutline a rdf:Property ;
    rdfs:label "Course Outline" ;
    rdfs:comment "Outline of the course."@en ;
    rdfs:domain course:Course ;
    rdfs:range foaf:Document .

course:courseSubject a rdf:Property ;
    rdfs:label "Course Subject" ;
    rdfs:comment "Subject of the Course."@en ;
    rdfs:domain course:Course ;
    rdfs:range rdfs:Literal .

course:hasLecture a rdf:Property ;
    rdfs:domain course:Course ;
    rdfs:range lecture:Lecture .

course:hasTopic a rdf:Property ;
    rdfs:label "has topic"@en ;
    rdfs:comment "Specifies the topic of a course"@en ;
    rdfs:domain course:Course ;
    rdfs:range topic:Topic .

course:offeredBy a rdf:Property ;
    rdfs:label "offered by"@en ;
    rdfs:comment "Specifies that a class is offered by a university"@en ;
    rdfs:domain course:Course ;
    rdfs:range university:University .

lecture:hasContent a rdf:Property ;
    rdfs:domain lecture:Lecture ;
    rdfs:range lectureContent:LectureContent .

lecture:lectureName a rdf:Property ;
    rdfs:domain lecture:lecture ;
    rdfs:range xsd:string .

lecture:lectureNumber a rdf:Property ;
    rdfs:domain lecture:lecture ;
    rdfs:range xsd:integer .

lectureContent:hasTopic a rdf:Property ;
    rdfs:label "has topic"@en ;
    rdfs:comment "Specifies the topic of a course"@en ;
    rdfs:domain lecture:Lecture ;
    rdfs:range topic:Topic .

student:Amish student:hasCompetency topic:KnowledgeGraph ;
    student:hasCompletedCourse completedCourse:COMP474Completion_Amish .

student:John student:hasCompetency topic:KnowledgeGraph ;
    student:hasCompletedCourse completedCourse:COMP474Completion_John .

student:hasCompetency a rdf:Property ;
    rdfs:domain student:Student ;
    rdfs:range topic:Topic .

student:hasCompletedCourse a rdf:Property ;
    rdfs:domain student:Student ;
    rdfs:range completedCourse:CompletedCourse .

student:hasGrade a rdf:Property ;
    rdfs:domain completedCourse:CompletedCourse .

topic:dbpediaEntry a rdf:Property ;
    rdfs:label "has DBpedia Entry" ;
    rdfs:comment "Link to topic DBpedia enrty."@en ;
    rdfs:domain topic:Topic ;
    rdfs:range rdfs:Resource .

topic:topicProvenanceInformation a rdf:Property ;
    rdfs:label "Provenance Information" ;
    rdfs:comment "record where the topic was identified as being covered in the course (e.g., the (graduate) calendar, the course outline or a specific lecture)."@en ;
    rdfs:domain topic:Topic ;
    rdfs:range rdfs:Literal .

topic:wikidataEntry a rdf:Property ;
    rdfs:label "has Wikidata Entry" ;
    rdfs:comment "Link to topic wikidata entry."@en ;
    rdfs:domain topic:Topic ;
    rdfs:range rdfs:Resource .

completedCourse:COMP474Completion_Amish a completedCourse:CompletedCourse,
        course:COMP474 ;
    student:completedOn "2022-05-15"^^xsd:date,
        "2023-05-15"^^xsd:date ;
    student:hasGrade "A",
        "F" .

completedCourse:COMP474Completion_John a completedCourse:CompletedCourse,
        course:COMP474 ;
    student:completedOn "2022-04-28"^^xsd:date,
        "2023-06-01"^^xsd:date ;
    student:hasGrade "B",
        "C" .

topic:Topic a rdfs:Topic ;
    rdfs:label "Topic Name" ;
    rdfs:comment "Topic names within a course outline."@en .

