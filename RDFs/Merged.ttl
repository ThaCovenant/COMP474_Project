@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix foaf1: <http://xmlns.com/foaf/.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wd: <https://wwww.wikidata.org/wiki/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

rdfs:Course a rdfs:Class ;
    rdfs:label "course" ;
    rdfs:comment "A course offered by the university."@en .

rdfs:Student a rdfs:Class ;
    rdfs:label "Student" ;
    rdfs:comment "This is a Student Class."@en .

rdfs:Other a rdf:Class ;
    rdfs:label "Other Materials" ;
    rdfs:comment "Other materials related to Lecture Content."@en ;
    rdfs:subClassOf rdfs:LectureContent .

rdfs:Readings a rdf:Class ;
    rdfs:label "Lecture Readings" ;
    rdfs:comment "Readings of lecture: book chapters, web pages, etc."@en ;
    rdfs:subClassOf rdfs:LectureContent .

rdfs:Slides a rdf:Class ;
    rdfs:label "Lecture Slides" ;
    rdfs:comment "Slides of lecture."@en ;
    rdfs:subClassOf rdfs:LectureContent .

rdfs:Worksheets a rdf:Class ;
    rdfs:label "Lecture Worksheets" ;
    rdfs:comment "Worksheets of lecture."@en ;
    rdfs:subClassOf rdfs:LectureContent .

rdfs:courseCredit a rdf:Property ;
    rdfs:label "Course Credits" ;
    rdfs:comment "The number of credits assigned to the course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range rdfs:Literal .

rdfs:courseDescription a rdf:Property ;
    rdfs:label "Course Description" ;
    rdfs:comment "Description of the course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range rdfs:Literal .

rdfs:courseLink a rdf:Property ;
    rdfs:label "Course Link to a webpage" ;
    rdfs:comment "Webpage of the course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range foaf:Document ;
    rdfs:seeAlso foaf:Document .

rdfs:courseName a rdf:Property ;
    rdfs:label "Course name" ;
    rdfs:comment "Name of the course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range rdfs:Literal .

rdfs:courseNumber a rdf:Property ;
    rdfs:label "Course number" ;
    rdfs:comment "Number of the course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range rdfs:Literal .

rdfs:courseOutline a rdf:Property ;
    rdfs:label "Course Outline" ;
    rdfs:comment "Outline of the course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range foaf:Document .

rdfs:courseSubject a rdf:Property ;
    rdfs:label "Course Subject" ;
    rdfs:comment "Subject of the Course."@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range rdfs:Literal .

rdfs:dbpediaEntry a rdf:Property ;
    rdfs:label "has DBpedia Entry" ;
    rdfs:comment "Link to topic DBpedia enrty."@en ;
    rdfs:domain rdfs:Topic ;
    rdfs:range rdfs:Resource .

rdfs:hasCompetency a rdf:Property ;
    rdfs:label "hasCompetency" ;
    rdfs:comment "Student has Completed."@en ;
    rdfs:domain rdfs:Student ;
    rdfs:range <file:///C:/Users/Amish/Documents/GitHub/366/COMP474_Project/Topics.ttl> .

rdfs:hasEmail a rdf:Property ;
    rdfs:label "hasEmail" ;
    rdfs:comment "Student Email."@en ;
    rdfs:domain rdfs:Student ;
    rdfs:range xsd:string .

rdfs:hasFirstName a rdf:Property ;
    rdfs:label "hasFirstName" ;
    rdfs:comment "Student First Name."@en ;
    rdfs:domain rdfs:Student ;
    rdfs:range xsd:string .

rdfs:hasGrade a rdf:Property ;
    rdfs:label "hasGrade" ;
    rdfs:comment "Student received a letter grade."@en ;
    rdfs:domain rdfs:hasCompletedCourse ;
    rdfs:range xsd:string .

rdfs:hasIDNumber a rdf:Property ;
    rdfs:label "hasIDNumber" ;
    rdfs:comment "Student ID."@en ;
    rdfs:domain rdfs:Student ;
    rdfs:range xsd:string .

rdfs:hasLastName a rdf:Property ;
    rdfs:label "hasLastName" ;
    rdfs:comment "Student Last Name."@en ;
    rdfs:domain rdfs:Student ;
    rdfs:range xsd:string .

rdfs:hasTopic a rdf:Property ;
    rdfs:label "has topic"@en ;
    rdfs:comment "Specifies the topic of a course"@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range <file:///C:/Users/Amish/Documents/GitHub/366/COMP474_Project/Topics.ttl> .

rdfs:lectureLink a rdf:Property ;
    rdfs:label "Lecture Link to a webpage" ;
    rdfs:comment "Webpage of the lecture."@en ;
    rdfs:domain rdfs:Lecture ;
    rdfs:range foaf1:Document ;
    rdfs:seeAlso foaf1:Document .

rdfs:lectureNumber a rdf:Property ;
    rdfs:label "lectureNumber" ;
    rdfs:comment "Sequential count of lecture within a course."@en ;
    rdfs:domain rdfs:Lecture ;
    rdfs:range xsd:integer .

rdfs:offeredBy a rdf:Property ;
    rdfs:label "offered by"@en ;
    rdfs:comment "Specifies that a class is offered by a university"@en ;
    rdfs:domain rdfs:Course ;
    rdfs:range <rdfs:../University_Schema.ttl> .

rdfs:topicProvenanceInformation a rdf:Property ;
    rdfs:label "Provenance Information" ;
    rdfs:comment "record where the topic was identified as being covered in the course (e.g., the (graduate) calendar, the course outline or a specific lecture)."@en ;
    rdfs:domain rdfs:Topic ;
    rdfs:range rdfs:Literal .

rdfs:wikidataEntry a rdf:Property ;
    rdfs:label "has Wikidata Entry" ;
    rdfs:comment "Link to topic wikidata entry."@en ;
    rdfs:domain rdfs:Topic ;
    rdfs:range rdfs:Resource .

rdfs:hasCompletedCourse a rdf:Property ;
    rdfs:label "hasCompletedCourse" ;
    rdfs:comment "Student has completed course."@en ;
    rdfs:domain rdfs:Student ;
    rdfs:range rdfs:Course .

rdfs:lectureContent a rdf:Property ;
    rdfs:label "Lecture Content" ;
    rdfs:comment "Content of lecture."@en ;
    rdfs:domain rdfs:Lecture ;
    rdfs:range rdfs:lectureContent .

rdfs:LectureContent a rdf:Class ;
    rdfs:label "Lecture Content" ;
    rdfs:comment "Lecture Content: Worksheets, Readings, Other..."@en ;
    rdfs:domain rdfs:Lecture ;
    rdfs:range foaf1:Document .

rdfs:Lecture a rdfs:Lecture ;
    rdfs:label "Lecture Name" ;
    rdfs:comment "Name of lecture, if available."@en .

rdfs:Topic a rdfs:Topic ;
    rdfs:label "Topic Name" ;
    rdfs:comment "Topic names within a course outline."@en .

