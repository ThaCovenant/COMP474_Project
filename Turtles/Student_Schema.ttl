@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix course: <http://example.org/vocab/course#>.
@prefix topic: <http://example.org/vocab/topic#>.


#Student class
rdfs:Student a rdfs:Class;
    rdfs:label "Student" ;
    rdfs:comment "This is a Student Class."@en.

#Properties
rdfs:hasFirstName a rdf:Property;
    rdfs:label "hasFirstName";
    rdfs:domain rdfs:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student First Name."@en.

rdfs:hasLastName a rdf:Property;
    rdfs:label "hasLastName";
    rdfs:domain rdfs:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student Last Name."@en.

rdfs:hasIDNumber a rdf:Property;
    rdfs:label "hasIDNumber";
    rdfs:domain rdfs:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student ID."@en.

rdfs:hasEmail a rdf:Property;
    rdfs:label "hasEmail";
    rdfs:domain rdfs:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student Email."@en.

rdfs:hasCompletedCourse a rdf:Property;
    rdfs:label "hasCompletedCourse";
    rdfs:domain rdfs:Student;
    rdfs:range course:Course;
    rdfs:comment "Student has completed course."@en.

rdfs:hasGrade a rdf:Property;
    rdfs:label "hasGrade";
    rdfs:domain rdfs:hasCompletedCourse;
    rdfs:range xsd:string;
    rdfs:comment "Student received a letter grade."@en.

rdfs:hasCompetency a rdf:Property;
    rdfs:label "hasCompetency";
    rdfs:domain rdfs:Student;
    rdfs:range topic:Topic;
    rdfs:comment "Student has Completed."@en.

#examples
#Person
rdfs:PhilippeBertrand a rdfs:Student;
    rdfs:hasFirstName "Philippe";
    rdfs:hasLastName "Bertrand";
    rdfs:hasIDNumber "11111";
    rdfs:hasEmail "emailexample@gmail.com";
    rdfs:hasCompletedCourse rdfs:COMP474;
    rdfs:hasGrade "A";
    rdfs:hasCompetency rdfs:KnowledgeGraphs.


rdfs:JimmyTran a rdfs:Student;
    rdfs:hasFirstName "Jimmy";
    rdfs:hasLastName "Tran";
    rdfs:hasIDNumber "40076313";
    rdfs:hasEmail "huy-longtran@hotmail.com";
    rdfs:hasCompletedCourse student:COMP472;
    rdfs:hasGrade "B-";
    rdfs:hasCompetency rdfs:KnowledgeGraphs.


rdfs:PhilippeBertrand rdfs:hasCompletedCourse rdfs:COMP474.
rdfs:COMP474 rdfs:hasGrade "A".

rdfs:RandomName a rdfs:Student;
    rdfs:hasFirstName "Random";
    rdfs:hasLastName "Name";
    rdfs:hasIDNumber "22222";
    rdfs:hasEmail "example@gmail.com";
    rdfs:hasCompletedCourse rdfs:COMP474;
    rdfs:hasGrade "B";
    rdfs:hasCompetency rdfs:KnowledgeGraphs.

rdfs:RandomName rdfs:hasCompletedCourse rdfs:COMP474.
rdfs:COMP474 rdfs:hasGrade "B".