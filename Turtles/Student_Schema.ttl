@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix course: <http://example.org/vocab/course#>.
@prefix topic: <http://example.org/vocab/topic#>.


#Student class
student:Student a rdfs:Class;
    rdfs:label "Student" ;
    rdfs:comment "This is a Student Class."@en.

#Properties
student:hasFirstName a rdf:Property;
    rdfs:label "hasFirstName";
    rdfs:domain student:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student First Name."@en.

student:hasLastName a rdf:Property;
    rdfs:label "hasLastName";
    rdfs:domain student:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student Last Name."@en.

student:hasIDNumber a rdf:Property;
    rdfs:label "hasIDNumber";
    rdfs:domain student:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student ID."@en.

student:hasEmail a rdf:Property;
    rdfs:label "hasEmail";
    rdfs:domain student:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student Email."@en.

student:hasCompletedCourse a rdf:Property;
    rdfs:label "hasCompletedCourse";
    rdfs:domain student:Student;
    rdfs:range course:Course;
    rdfs:comment "Student has completed course."@en.

student:hasGrade a rdf:Property;
    rdfs:label "hasGrade";
    rdfs:domain student:Student;
    rdfs:range xsd:string;
    rdfs:comment "Student received a letter grade."@en.

student:hasCompetency a rdf:Property;
    rdfs:label "hasCompetency";
    rdfs:domain student:Student;
    rdfs:range topic:Topic;
    rdfs:comment "Student has Completed."@en.

#examples
#Person
student:PhilippeBertrand a student:Student;
    student:hasFirstName "Philippe";
    student:hasLastName "Bertrand";
    student:hasIDNumber "11111";
    student:hasEmail "emailexample@gmail.com";
    student:hasCompletedCourse student:COMP474;
    student:hasGrade "A";
    student:hasCompetency student:KnowledgeGraphs.
