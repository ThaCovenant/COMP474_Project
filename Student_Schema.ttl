@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://example.org/>.

#Student class
ex:Student a rdfs:Class.

#Properties
ex:hasFirstName a rdf:Property; rdfs:label "hasFirstName"; rdfs:domain ex:Student.
ex:hasLastName a rdf:Property; rdfs:label "hasLastName"; rdfs:domain ex:Student.
ex:hasIDNumber a rdf:Property; rdfs:label "hasIDNumber"; rdfs:domain ex:Student.
ex:hasEmail a rdf:Property; rdfs:label "hasEmail"; rdfs:domain ex:Student.
ex:hasCompletedCourse a rdf:Property; rdfs:label "hasCompletedCourse"; rdfs:domain ex:Student.
ex:hasGrade a rdf:Property; rdfs:label "hasGrade"; rdfs:domain ex:Student.
ex:hasCompetency a rdf:Property; rdfs:label "hasCompetency"; rdfs:domain ex:Student.

#examples
#Person
ex:PhilippeBertrand a ex:Student;
    ex:hasFirstName "Philippe";
    ex:hasLastName "Bertrand";
    ex:hasIDNumber "11111";
    ex:hasEmail "emailexample@gmail.com";
    ex:hasCompletedCourse ex:COMP474;
    ex:hasGrade ex:A;
    ex:hasCompetency ex:Knowledge Graphs.

ex:Course1 a ex:Course;
    rdfs:label "COMP474";
    ex:includesTopic "Knowledge Graphs".

ex:Grade1 a ex:Grade;
    rdfs:label "A".

ex:Competency1 a ex:Competency;
    ex:competentInTopic "Knowledge Graphs".



