    rdfs:dbpediaEntry <https://dbpedia.org/resource/Deterministic_Finite_Automata>.

ex:COMP474 a rdfs:Course ;
    rdfs:hasCourseName "Intelligent Systems" ;
    rdfs:hasSubject "COMP" ;
    rdfs:hasCourseNumber "474" ;
    rdfs:hasCredits 3 ;
    rdfs:offeredBy "Concordia" ;
    rdfs:hasTopic "Blob" ;
