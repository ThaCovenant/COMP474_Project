#lectures
ex:IntroductiontoTheoryofComputation a rdfs:Lecture;
    rdfs:label "Introduction to Theory of Computation";
    rdfs:LectureNumber 1;
    rdfs:LectureContent "Theory of Computation";
    rdfs:comment "First lecture of the class."@en;
    rdfs:hasTopic ex:TheoryofComputation.

ex:DeterministicFiniteStateAutomata a rdfs:Lecture;
    rdfs:label "(Deterministic) Finite State Automata";
    rdfs:LectureNumber 2;
    rdfs:LectureContent "Finite State Automata";
    rdfs:comment "Second lecture of the class."@en;
    rdfs:hasTopic ex:DeterministicFiniteStateAutomata.

#topics
ex:TheoryofComputation a rdfs:Topic;
    rdfs:label "Theory of Computation";
    rdfs:comment "This topic was covered in the first lecture."@en;
    rdfs:topicProvenanceInformation "Course Outline";
    rdfs:wikidataEntry <https://www.wikidata.org/entity/Q844718>;
    rdfs:dbpediaEntry <https://dbpedia.org/resource/Theory_of_Computation>.

ex:DeterministicFiniteStateAutomata a rdfs:Topic;
    rdfs:label "(Deterministic) Finite State Automata";
